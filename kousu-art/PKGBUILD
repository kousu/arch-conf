pkgname=kousu-art
pkgdesc="Configuration management for kousu's creative world"

# load common settings; be aware if you ever split up this repo.
[ -f ../lib/lib.PKGBUILD ] && cp -lf ../lib/lib.PKGBUILD .lib.PKGBUILD
. .lib.PKGBUILD || exit 1

depends=(
  kousu-art-audio
  kousu-art-graphics
  kousu-art-video
)
