PRESETS=('default' 'fallback')

ALL_kver="/boot/vmlinuz-linux"

default_uki="/boot/efi/boot/bootx64.efi"
# To change the boot logo, make sure your new logo is:
# smaller than your screen resolution and in .bmp format.
# (this is only respected with with the EFI image)
default_options="--splash=/etc/bootsplash.bmp"

fallback_uki="/boot/efi/boot/archlinux-linux-fallback.efi"
fallback_options="-S autodetect"
